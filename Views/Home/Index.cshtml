@using Microsoft.AspNet.Identity
@model Dejtinghemsida.Models.IndexViewModel
@{

    if (HttpContext.Current.User.Identity.GetUserId() != null)
    {
        ViewBag.Title = "Startsida";
        Layout = "~/Views/Shared/_SignedInLayout.cshtml";

    }
}
<div class="jumbotron">
    <h1 class="display-3">MötesPlats</h1>
    <p class="lead">Sveriges bästa dejtingsajt.</p>
    

</div>
<div class="row">
    @foreach (var item in Model.ExampleProfiles)
    {
        <div class="card mb-4 mx-1 col-sm">
            <h3 class="card-header">@Html.DisplayFor(m => item.Förnamn)  </h3>
            <div class="card-body">
                <h5 class="card-title">@Html.DisplayFor(m => item.Kön), @Html.DisplayFor(m => item.Ålder)</h5>
            </div>

            <img src="~/Images/hejhej.jpg" style="width:150px;height:150px; background: #808080;
                margin: auto;
                -moz-border-radius: 50px;
                border-radius: 100px;
                padding: 3px;
               " />

            <div class="card-body">


                <a href="/Profiles/OtherProfile/@item.Id" class="card-link" style="float:left"> Min profil</a>

            </div>

        </div>
    }
</div>



